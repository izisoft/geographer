{
    "name": "izisoft/geographer",
    "type": "library",
    "description": "A PHP library that knows how any country, state or city is called in any language",
    "keywords": ["geolocation","localization","geonames","geo"],
    "homepage": "https://github.com/MenaraSolutions/geographer",
    "license": "MIT",
    "authors": [
        {
            "name": "A Tin",
            "email": "zinzinx8@gmail.com",
            "homepage": "https://www.menara.com.au"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "izisoft/geographer-data": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*|5.*",
        "codeclimate/php-test-reporter": "dev-master",
        "illuminate/support": "5.*"
    },
    "autoload": {
        "psr-4": {
            "MenaraSolutions\\Geographer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MenaraSolutions\\Geographer\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.4-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
